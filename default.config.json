{
  "use": {
    "scoop": "false"
  },
  "relPathsTo": {
    "applications": "../_portable-applications",
    "binaries": "../_portable-binaries",
    "shortcuts": "../_portable-shortcuts",
    "cmderConfig": "../_portable-applications/cmder/config",
    "sourceToAccessHooks": "../_portable-scripts/custom/script.ps1"
  },
  "aliases": [
    {
      "name": "e.",
      "value": "explorer .",
      "use": ["cmd"]
    },
    {
      "name": "gl",
      "value": "git log --oneline --all --graph --decorate  $*",
      "use": ["cmd"]
    },
    {
      "name": "ls",
      "value": "ls --show-control-chars -F --color $*",
      "use": ["cmd"]
    },
    {
      "name": "pwd",
      "value": "cd",
      "use": ["cmd"]
    },
    {
      "name": "clear",
      "value": "cls",
      "use": ["cmd"]
    },
    {
      "name": "history",
      "value": "cat \"%CMDER_ROOT%\\config\\.history\"",
      "use": ["cmd"]
    },
    {
      "name": "unalias",
      "value": "alias /d $1",
      "use": ["cmd"]
    },
    {
      "name": "vi",
      "value": "vim $*",
      "use": ["cmd"]
    },
    {
      "name": "cmderr",
      "value": "cd /d \"%CMDER_ROOT%\"",
      "use": ["cmd"]
    },
    {
      "name": "gr",
      "value": "gradle run",
      "use": ["cmd"]
    },
    {
      "name": "g",
      "value": "clear",
      "use": ["cmd"]
    },
    {
      "name": "la",
      "value": "'ls -al'",
      "use": ["cmd"]
    },
    {
      "name": "ll",
      "value": "ls -al",
      "use": ["cmd"]
    }
  ]
}