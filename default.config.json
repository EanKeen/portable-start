{
  "relPathsTo": {
    "applications": "../_portable-applications",
    "binaries": "../_portable-binaries",
    "shortcuts": "../_portable-shortcuts",
    "cmderConfig": "../applications/cmder/config",
    "sourceToAccessHooks": "../_portable-extend.ps1"
  },
  "aliases": [
    {
      "name": "e.",
      "value": "explorer .",
      "use": ["cmd"]
    },
    {
      "name": "gl",
      "value": "git log --oneline --all --graph --decorate  $*"
    },
    {
      "name": "ls",
      "value": "ls --show-control-chars -F --color $*"
    },
    {
      "name": "pwd",
      "value": "cd",
      "use": ["cmd"]
    },
    {
      "name": "clear",
      "value": "cls",
      "use": ["cmd"]
    },
    {
      "name": "history",
      "value": "cat \"%CMDER_ROOT%\\config\\.history\"",
      "use": ["cmd"]
    },
    {
      "name": "unalias",
      "value": "alias /d $1",
      "use": ["cmd"]
    },
    {
      "name": "vi",
      "value": "vim $*"
    },
    {
      "name": "cmderr",
      "value": "cd /d \"%CMDER_ROOT%\"",
      "use": ["cmd"]
    },
    {
      "name": "gr",
      "value": "gradle run"
    },
    {
      "name": "g",
      "value": "clear"
    },
    {
      "name": "la",
      "value": "'ls -al'"
    },
    {
      "name": "ll",
      "value": "ls -al"
    }
  ]
}